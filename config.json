{
    "path": {
        "src": {
            "root": "src",
            "asset": {
                "javascript": "src/asset/javascript",
                "image": "src/asset/image",
                "font": "src/asset/font",
                "scss": "src/asset/scss"
            },
            "prototype": {
                "template": "src/prototype/template",
                "data": "src/prototype/data",
                "webroot": "src/prototype/webroot"
            },
            "patternLibrary": {
                "root": "node_modules/frontend-incubator-kss-template/build",
                "template": "node_modules/frontend-incubator-kss-template/build"
            }
        },

        "build": {
            "root": "build",
            "asset": {
                "javascript": "build/asset/javascript",
                "image": "build/asset/image",
                "font": "build/asset/font",
                "css": "build/asset/css"
            },
            "prototype": {
                "template": "build/prototype",
                "webroot": "build/prototype"
            },
            "patternLibrary": {
                "root": "build/pattern_library"
            },
            "site": "build/prototype"
        },

        "buildUrl": {
            "site": "/",
            "css": "/css",
            "javascript": "/asset/javascript",
            "image": "/asset/image",
            "font": "/asset/font"
        }
    },
    "ftpDeploy": {
        "host": "",
        "password": "",
        "username": ""
    },
    "preferences": {
        "autoprefix": {
            "support": "last 2 versions, Explorer >= 8, Firefox >= 25"
        },
        "es2015": [
          "es2015", {
          "loose": true
        }]
    }
}
